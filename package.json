{
  "name": "@sersol/ngx-leaflet",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --ssl --ssl-key localhost.key  --ssl-cert localhost.crt",
    "start-plain": "ng serve -o",
    "build": "ng build ngx-leaflet --configuration production",
    "publish-lib": "npm run build && npm publish ./dist --access public && npm run publish-doc && npm run publish-showcase",
    "publish-doc": "npx compodoc -p tsconfig.json -d ../ngx-doc --theme vagrant --language es-ES src",
    "commit": "git --C=../../ngx-doc-leaflet commit -m 'Auto publish' && git --C=../../ngx-doc-leaflet push",
    "publish-showcase": "ng deploy showcase --base-href=/ngx-leaflet/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^13.0.2",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/forms": "^13.0.2",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "@angular/router": "^13.0.2",
    "@sersol/ngx": "^0.40.0",
    "angular-cli-ghpages": "^1.0.0",
    "bowser": "^2.11.0",
    "highlight.js": "^11.3.1",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.3",
    "ngx-highlightjs": "^6.1.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1300.3",
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular/cli": "^13.0.3",
    "@angular/compiler-cli": "^13.0.2",
    "@angular/language-service": "^13.0.2",
    "@types/highlight.js": "^10.1.0",
    "@types/jasmine": "~3.3.16",
    "@types/leaflet": "^1.7.6",
    "@types/leaflet.markercluster": "^1.4.6",
    "@types/node": "^12.20.15",
    "@types/facebook-js-sdk": "^3.3.2",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.54",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.7",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.1",
    "typescript": "~4.4.4"
  }
}
